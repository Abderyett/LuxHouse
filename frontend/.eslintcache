Product } from '../actions/productActions';\nimport { incrementCount, decrementCount } from '../actions/cartAction';\n\nexport function SingleProduct() {\n  const { id } = useParams();\n  const productDetail = useSelector((state) => state.productDetail);\n  const num = useSelector((state) => state.cart);\n  const dispatch = useDispatch();\n\n  const [selectedPhoto, setselectedPhoto] = useState(null);\n  const { loading, error, product } = productDetail;\n\n  useEffect(() => {\n    dispatch(detailProduct(id));\n  }, [dispatch]);\n\n  return (\n    <>\n      <Header />\n      {loading ? (\n        <Loader />\n      ) : error ? (\n        <Message bg=\"danger\">{error}</Message>\n      ) : (\n        <Container>\n          <FirstSection>\n            <PhotosSection>\n              <ImageZoom>\n                <SelectedImg img={selectedPhoto === null ? product.images && product.images[0].url : selectedPhoto} />\n              </ImageZoom>\n              <ImgsWrapper>\n                {product.images &&\n                  product.images.map((p) => <IMG key={p._id} src={p.url} onClick={() => setselectedPhoto(p.url)} />)}\n              </ImgsWrapper>\n            </PhotosSection>\n            <DetailsSection>\n              <DetailWrapper>\n                <DetailHeader>\n                  <Price>\n                    <h3>{product && product.name}</h3>\n                    <p>{product && formatter.format(product.price)}</p>\n                  </Price>\n                  <Text>{product && product.subcategory}</Text>\n                  <DescriptionText>\n                    {product && product.description && product.description.substring(0, 149)}\n                  </DescriptionText>\n                </DetailHeader>\n                <ColorWrapper>\n                  <b>{product && product.colors && 'Textures/Colors styles :'}</b>\n                  {product && product.colors && product.colors.map((c, index) => <Color key={index} texture={c} />)}\n                </ColorWrapper>\n\n                <ButtonWrapper>\n                  <div>\n                    {product && product.shipping === 'true' ? <FiCheckCircle /> : <BsXCircle />} &nbsp;\n                    <span>{product && product.shipping === 'true' ? 'Available' : 'Not available'} for delivery</span>\n                  </div>\n\n                  {/* !Cart buttons */}\n\n                  <CartBtn>\n                    <Heart />\n                    <Button>\n                      {' '}\n                      Add to cart\n                      <span onClick={() => dispatch(incrementCount())}>\n                        <PlusIcon />\n                      </span>{' '}\n                      &nbsp; {num.count}\n                      <span onClick={() => dispatch(decrementCount())}>\n                        &nbsp;\n                        <MinusIcon />\n                      </span>\n                    </Button>\n                  </CartBtn>\n\n                  {/* !Cart buttons */}\n                </ButtonWrapper>\n              </DetailWrapper>\n            </DetailsSection>\n          </FirstSection>\n          <SecondSection>\n            <Description>\n              <h4>Description</h4>\n              <p> {product && product && product.description}</p>\n            </Description>\n            <Features>\n              {' '}\n              <h4>Features</h4>\n              <FirstFeature>\n                {' '}\n                <Equipement /> <span>{product && product.Features && product.Features[0]}</span>\n              </FirstFeature>\n              <SecondFeature>\n                {' '}\n                <Dimension /> <span>{product && product.Features && product.Features[1]}</span>\n              </SecondFeature>\n            </Features>\n          </SecondSection>\n        </Container>\n      )}\n    </>\n  );\n}\nconst Container = styled.div``;\n\nconst FirstSection = styled.section`\n  height: 70vh;\n  display: grid;\n  grid-auto-flow: column;\n  grid-template-columns: 5