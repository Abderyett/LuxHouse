t [filtredProducts, setfiltredProducts] = useState([]);\n  const [categorieText, setCategorieText] = useState('all');\n\n  useEffect(() => {\n    dispatch(listProduct());\n  }, [dispatch]);\n\n  useEffect(() => {\n    if (products.length > 0) {\n      const newArr = products.map((el) => el.subcategory);\n      const categories = ['all', ...new Set(newArr)];\n      setsubCategories(categories);\n    }\n  }, [products]);\n\n  const filterProducts = (category) => {\n    if (category === 'all') {\n      setfiltredProducts(products);\n    } else {\n      const newProducts = products.filter((el) => el.subcategory === category);\n      setfiltredProducts(newProducts);\n    }\n  };\n\n  useEffect(() => {\n    filterProducts(categorieText);\n  }, [categorieText]);\n\n  return (\n    <>\n      <Header productList />\n      {loading ? (\n        <Loader />\n      ) : error ? (\n        <Message bg=\"danger\">{error}</Message>\n      ) : (\n        <Container>\n          <Div>\n            <CategorieWrapper>\n              {subCategories.map((el, index) => (\n                <Button key={index} type=\"button\" onClick={(e) => setCategorieText(e.target.textContent.toLowerCase())}>\n                  {el}\n                </Button>\n              ))}\n            </CategorieWrapper>\n          </Div>\n          <ContentWrapper>\n            <FilterWrapper>\n              <Accordion />\n\n              <ClearBtn type=\"button\">Clear Filters</ClearBtn>\n            </FilterWrapper>\n            <div>\n              <HeaderDetails>\n                <span>{filtredProducts.length === 0 ? products.length : filtredProducts.length} Results </span>\n                <hr /> <span>Sort By</span>\n              </HeaderDetails>\n\n              <GridContainer>\n                {categorieText === 'all'\n                  ? products.map((el, i) => (\n                      <Card key={el._id}>\n                        <Link to={`/products/${el._id}`}>\n                          <StyledImg src={el.image[0].url} alt={el.subcategory} />\n                          <Title>\n                            <h3>{el.name}</h3>\n                            <PriceText category={el.category}>${el.price}</PriceText>\n                          </Title>\n                          <Subcategory category={el.category}>{el.subcategory} </Subcategory>\n                          <Description>{el.description.substring(0, 100)}...</Description>\n                        </Link>\n                        <IconWrapper type=\"button\" onClick={() => dispatch(addItem(el))}>\n                          {isAdded && isAdded[i] ? <CheckCircle /> : <BlueCircle />}\n                        </IconWrapper>\n                      </Card>\n                    ))\n                  : filtredProducts.map((el, index) => (\n                      <Card key={el._id}>\n                        <Link to={`/products/${el._id}`}>\n                          <StyledImg src={el.image[0].url} alt={el.subcategory} />\n                          <Title>\n                            <h3>{el.name}</h3>\n                            <PriceText category={el.category}>${el.price}</PriceText>\n                          </Title>\n                          <Subcategory category={el.category}>{el.subcategory}</Subcategory>\n                          <Description>{el.description.substring(0, 100)}...</Description>\n                        </Link>\n                        <IconWrapper style={{ background: 'red' }} type=\"submit\" onClick={() => dispatch(addItem(el))}>\n                          {isAdded && isAdded[index] ? <CheckCircle /> : <BlueCircle />}\n                        </IconWrapper>\n                      </Card>\n                    ))}\n              </GridContainer>\n            </div>\n          </ContentWrapper>\n        </Container>\n      )}\n    </>\n  );\n}\n\nconst Container = styled.div`\n  background-color: rgba(240, 244, 248, 0.5);\n  height: 100%;\n  padding-bottom: 5rem;\n`;\nconst ContentWrapper = s